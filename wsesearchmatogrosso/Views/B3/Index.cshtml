        $('.treemapb3').css('display', 'none');
        $('.chartsLineb3').css('display', 'none');
        $('.chartsRadarb3').css('display', 'none');
        $('.graficoQtdb3').css('display', 'block');
        $('.chartsBubleb3').css('display', 'none');
        $('.chartsBarb3').css('display', 'none');
        $('.searchb3').css('display', 'none');
        $('.scrolls').css('display', 'block');
    }

