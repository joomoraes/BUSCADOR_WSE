<script>

    function camelSentence(str) {
        return (" " + str).toLowerCase().replace(/[^a-zA-Z0-9]+(.)/g, function (match, chr) {
            return chr.toUpperCase();
        });
    };

    function searchB3() {
        let data = camelSentence($('#buscaEmpresab3').val());
        let dataselect = $('#exampleFormControlSelect1').val();



        $.ajax({
            url: 'SearchDataB3',
            data:
            {
                pesquisa: data,
                tipo: dataselect
            },
            method: 'Post',
            dataType: 'Html',
            cache: false,
            success: function (result) {
                $('#Rpesquisa').html(result);
            }
        });
    }

    function Empresas() {
        $.ajax({
            url: 'Empresas',
            cache: false,
            method: 'Post',
            success: function (result) {
                swal({
                    title: "Empresas Cadastradas Na Bolsa",
                    text: result,
                });
            }
        })
    }

